{% extends 'component/ui_popup_base.jinja2' %}

{% block popup_id %}popupDeleteConfirm{% endblock %}

{% block popup_content %}
<form class="form-center">
    {{ forms.form_center_text(_('CONFIRM DELETE')) }}
    <div class="form-center-buttons">
        {{ forms.button('deleteFileConfirm', _('CONFIRM BUTTON')) }}
        {{ forms.warn_button('deleteFileCancel', _('CANCEL BUTTON')) }}
    </div>
</form>
{% endblock %}